/**** Start of imports. If edited, may not auto-convert in the playground. ****/
var geometry = /* color: #d63000 */ee.Geometry.Polygon(
        [[[-60.82949200103815, -36.01412282226637],
          [-58.77504864166315, -36.44835825309486],
          [-60.23623028228815, -35.26412940071591]]]);
/***** End of imports. If edited, may not auto-convert in the playground. *****/

var dataset = ee.ImageCollection('MODIS/006/MCD15A3H').filterBounds(geometry).filterDate('2019-01-01', '2020-12-31');

var fapar = dataset.select('Fpar');
var lai = dataset.select('Lai');
var mergedCollection = fapar.merge(lai);

var title = {
  title: 'Evoluci√≥n fAPAR & LAI',
  hAxis: {title: 'Tiempo'},
  vAxis: {title: 'Valor'},
};

print(ui.Chart.image.series(mergedCollection, geometry).setOptions(title));

